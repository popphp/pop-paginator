{
    "name": "popphp/pop-paginator",
    "description": "Pop Paginator Component for Pop PHP Framework",
    "keywords": [
        "paginator",
        "pagination",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Paginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Paginator\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    }
}
